<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.carlos.proyecto2ab.gastos"
    android:background="#b0acac">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Comida"
        android:id="@+id/textView16"
        android:textSize="16dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="44dp"
        android:textColor="#070707" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText10"
        android:layout_alignTop="@+id/textView16"
        android:layout_centerHorizontal="true"
        android:textColor="#ea121d"
        android:textColorLink="#a60505" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Transporte"
        android:id="@+id/textView17"
        android:textSize="16dp"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/editText10"
        android:layout_alignParentStart="true"
        android:textColor="#070707" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText11"
        android:layout_alignTop="@+id/textView17"
        android:layout_toEndOf="@+id/textView17"
        android:textColor="#ea121d" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Creditos"
        android:id="@+id/textView18"
        android:textSize="16dp"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:textColor="#070707" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText12"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/editText13"
        android:textColor="#ea121d" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Diversion"
        android:id="@+id/textView19"
        android:textSize="16dp"
        android:layout_below="@+id/editText12"
        android:layout_alignParentStart="true"
        android:layout_marginTop="44dp"
        android:textColor="#070707" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText13"
        android:layout_alignTop="@+id/textView19"
        android:layout_alignStart="@+id/editText11"
        android:textColor="#ea121d" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Guardar"
        android:id="@+id/button5"
        android:layout_below="@+id/editText13"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:onClick="guardar" />

</RelativeLayout>
